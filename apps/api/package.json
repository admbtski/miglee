{
  "name": "@miglee/api",
  "version": "1.0.0",
  "private": true,
  "description": "GraphQL API server with Fastify and Prisma",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "gql:gen": "graphql-codegen --config src/graphql/codegen.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:format": "prisma format",
    "prisma:push": "prisma db push",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:reset": "prisma migrate reset",
    "prisma:force:reset": "prisma db push --force-reset",
    "worker:reminders:dev": "tsx watch src/workers/reminders/worker.ts",
    "worker:reminders": "tsx src/workers/reminders/worker.ts",
    "worker:feedback:dev": "tsx watch src/workers/feedback/worker.ts",
    "worker:feedback": "tsx src/workers/reminders/feedback.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.934.0",
    "@aws-sdk/s3-request-presigner": "^3.934.0",
    "@bull-board/api": "^6.15.0",
    "@bull-board/fastify": "^6.15.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^10.0.0",
    "@fastify/multipart": "^9.3.0",
    "@fastify/otel": "^0.9.4",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.3",
    "@graphql-tools/schema": "^10.0.25",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.205.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.205.0",
    "@opentelemetry/instrumentation-fastify": "^0.50.0",
    "@opentelemetry/instrumentation-graphql": "^0.53.0",
    "@opentelemetry/instrumentation-http": "^0.205.0",
    "@opentelemetry/instrumentation-ioredis": "^0.53.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@paralleldrive/cuid2": "^3.0.4",
    "@prisma/client": "^6.16.2",
    "@prisma/instrumentation": "^6.16.2",
    "blurhash": "^2.0.5",
    "bullmq": "^5.61.2",
    "dataloader": "^2.2.3",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.0.1",
    "fastify-raw-body": "^5.0.0",
    "graphql": "^16.11.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^5.8.0",
    "maplibre-gl": "^5.11.0",
    "mercurius": "^16.3.0",
    "mqemitter-redis": "^7.1.0",
    "nanoid": "^5.1.6",
    "pino": "^9.11.0",
    "resend": "^6.5.2",
    "sharp": "^0.34.5",
    "stripe": "^20.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@graphql-codegen/typescript-resolvers": "^5.0.0",
    "@miglee/config": "workspace:*",
    "@types/graphql-depth-limit": "^1.1.6",
    "@types/node": "^24.5.2",
    "eslint": "^9.36.0",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.16.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
