{
  "name": "@appname/api",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "GraphQL API server with Fastify and Prisma",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsdown",
    "build:tsc": "tsc && pnpm build:copy-schema",
    "build:copy-schema": "mkdir -p dist && cp ../../packages/contracts/graphql/schema.graphql dist/schema.graphql",
    "start": "node dist/index.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "gql:gen": "graphql-codegen --config src/graphql/codegen.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:format": "prisma format",
    "prisma:push": "prisma db push",
    "prisma:seed": "tsx prisma/seed-dev.ts",
    "prisma:seed:dev": "tsx prisma/seed-dev.ts",
    "prisma:seed:prod": "tsx prisma/seed-prod.ts",
    "prisma:reset": "prisma migrate reset",
    "prisma:force:reset": "prisma db push --force-reset",
    "worker:reminders:dev": "tsx watch src/workers/reminders/worker.ts",
    "worker:reminders": "tsx src/workers/reminders/worker.ts",
    "worker:feedback:dev": "tsx watch src/workers/feedback/worker.ts",
    "worker:feedback": "tsx src/workers/feedback/worker.ts",
    "worker:audit-archive:dev": "tsx watch src/workers/audit-archive/worker.ts",
    "worker:audit-archive": "tsx src/workers/audit-archive/worker.ts"
  },
  "dependencies": {
    "@appname/contracts": "workspace:*",
    "@aws-sdk/client-s3": "^3.934.0",
    "@aws-sdk/s3-request-presigner": "^3.934.0",
    "@bull-board/api": "^6.15.0",
    "@bull-board/fastify": "^6.15.0",
    "@fastify/compress": "^8.3.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^10.0.0",
    "@fastify/multipart": "^9.3.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.3",
    "@graphql-tools/schema": "^10.0.25",
    "@paralleldrive/cuid2": "^3.0.4",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@prisma/instrumentation": "^6.16.2",
    "blurhash": "^2.0.5",
    "bullmq": "^5.61.2",
    "dataloader": "^2.2.3",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.0.1",
    "fastify-raw-body": "^5.0.0",
    "graphql": "^16.11.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^5.8.0",
    "maplibre-gl": "^5.11.0",
    "mercurius": "^16.6.0",
    "mqemitter-redis": "^7.1.0",
    "nanoid": "^5.1.6",
    "pg": "^8.16.3",
    "pino": "^9.11.0",
    "resend": "^6.5.2",
    "sharp": "^0.34.5",
    "stripe": "^20.0.0"
  },
  "devDependencies": {
    "@appname/config": "workspace:*",
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@graphql-codegen/typescript-resolvers": "^5.0.0",
    "@types/graphql-depth-limit": "^1.1.6",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.16.0",
    "@types/ws": "^8.18.1",
    "eslint": "^9.36.0",
    "pino-pretty": "^13.1.1",
    "prisma": "^7.2.0",
    "tsdown": "^0.18.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "valibot": "^1.2.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed-dev.ts"
  },
  "volta": {
    "node": "24.12.0",
    "pnpm": "10.27.0"
  }
}
