{
  "name": "miglee",
  "version": "1.0.0",
  "private": true,
  "description": "MVP monorepo with React, Next.js, Fastify, and GraphQL",
  "scripts": {
    "dev": "clear & turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "gql:gen": "turbo run gql:gen",
    "db:up": "docker-compose -f docker/docker-compose.dev.yml up -d",
    "db:down": "docker-compose -f docker/docker-compose.dev.yml down",
    "db:logs": "docker-compose -f docker/docker-compose.dev.yml logs -f"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13"
  },
  "engines": {
    "node": ">=22.18.0",
    "pnpm": ">=10.14.0"
  },
  "volta": {
    "node": "22.18.0",
    "pnpm": "10.14.0"
  },
  "packageManager": "pnpm@10.14.0"
}
